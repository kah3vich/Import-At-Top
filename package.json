{
	"name": "import-at-top",
	"publisher": "kah3vich",
	"displayName": "Import At Top (Beta)",
	"description": "An extension that adds imports to the source file from the specified options in the configuration file, and also sorts and removes unnecessary imports, which improves and speeds up the user experience.",
	"repository": {
		"type": "git",
		"url": "https://github.com/kah3vich/Import-At-Top"
	},
	"version": "0.0.31",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other",
		"Formatters",
		"Linters",
		"Debuggers"
	],
	"preview": false,
	"galleryBanner": {
		"color": "#ffffff",
		"theme": "light"
	},
	"main": "./out/src/extension.js",
	"browser": "./out/src/extension.js",
	"contributes": {
		"configuration": {
			"title": "✅ Import At Top",
			"properties": {
				"import-at-top.formatter": {
					"type": "object",
					"description": "✅ Formatter params - Import At Top",
					"default": {
						"printWidth": 120,
						"tabWidth": 4,
						"useTabs": true,
						"semi": true,
						"bracketSpacing": true,
						"bracketSameLine": false,
						"jsxBracketSameLine": false,
						"singleQuote": true
					}
				},
				"import-at-top.config": {
					"type": "array",
					"description": "✅ Config params - Import At Top",
					"default": [
						{
							"triggerDefault": [
								"React"
							],
							"triggerExport": [
								"useState",
								"useId",
								"useRef"
							],
							"package": "react"
						},
						{
							"triggerDefault": [],
							"triggerExport": [
								"createStore"
							],
							"package": "redux"
						}
					]
				}
			}
		},
		"commands": [
			{
				"command": "import-at-top",
				"title": "Import At Top"
			}
		],
		"keybindings": [
			{
				"command": "import-at-top",
				"key": "ctrl+alt+s",
				"when": "editorTextFocus",
				"mac": "command+option+s"
			}
		]
	},
	"activationEvents": [
		"onCommand:import-at-top"
	],
	"icon": "assets/logo.png",
	"license": "Import At Top License",
	"author": {
		"name": "kah3vich",
		"url": "https://github.com/kah3vich"
	},
	"bugs": {
		"url": "https://github.com/kah3vich/Import-At-Top/issues"
	},
	"keywords": [
		"import",
		"react",
		"typescript",
		"formatted",
		"formatting",
		"format",
		"sort",
		"javascript",
		"linter",
		"other",
		"top",
		"auto"
	],
	"homepage": "https://github.com/kah3vich/Import-At-Top/readme.md",
	"scripts": {
		"build": "rm -rf out/ & clear & vsce package",
		"vscode:prepublish": "tsc -p ./",
		"watcher": "tsc -watch -p ./",
		"test": "rm -rf out/ & clear & jest --config ./jest.config.js",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/jest": "^29.4.0",
		"@types/mocha": "^8.2.2",
		"@types/node": "^6.0.40",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"@vscode/test-electron": "^1.6.1",
		"glob": "^7.1.7",
		"husky": "^8.0.3",
		"jest": "^29.4.3",
		"mocha": "^8.4.0",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"@types/eslint": "^8.21.0",
		"@types/prettier": "^2.7.2",
		"eslint": "^7.32.0",
		"prettier": "^2.8.4"
	}
}
